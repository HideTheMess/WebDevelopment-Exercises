<!DOCTYPE html>
<html>
<head>
</head>
<body>
	<table>
		<tr>

			<th> Request ID </th>
			<th> Cat ID </th>
			<th> Begin Date </th>
			<th> End Date </th>
			<th> Status </th>

		</tr>

		<% @requests.each do |request| %>
		<tr>

			<td> <%= request.id %> </td>
			<td> <%= request.cat_id %> </td>
			<td> <%= request.begin_date.strftime('%m-%d-%Y') %> </td>
			<td> <%= request.end_date.strftime('%m-%d-%Y') %> </td>
			<td> <%= request.status %> </td>

		</tr>
		<% end %>

	</table>
</body>
</html>